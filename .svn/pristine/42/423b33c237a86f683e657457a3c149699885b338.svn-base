.nav-tabs .nav-link-censo {
	color: #333;
}
.nav-tabs .nav-link-censo.active {
	color: #000;
    background-color: #ccc;
}
.divTextObs {
	display: block;
	width: 100%;
	height: 140px;
	background-color: #e9ecef;
	margin: 0;
	padding: .25rem .5rem;
	line-height: 1.5;
	border: 1px solid #ced4da;
	border-radius: .2rem;
	opacity: 1;
	font-size: .75rem;
	font-weight: 400;
	color: #495057
	background-clip: padding-box;
	transition: none;
}
.divTextObs h1,h2,h3,h4,h5,h6,p {
	margin-bottom: .2rem;
}
.divTextObs h1 { font-size: 1.2rem; }
.divTextObs h2 { font-size: 1.1rem; }
.divTextObs h3 { font-size: 1.0rem; }
.divTextObs h4 { font-size: .95rem; }
.divTextObs h5 { font-size: .90rem; }
.divTextObs h6 { font-size: .85rem; }