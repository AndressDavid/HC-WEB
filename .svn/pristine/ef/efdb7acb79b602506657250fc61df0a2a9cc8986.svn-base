<div id="divProcedimientosCTC" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="divProcedimientosCTC" data-backdrop="static">
	<div class="modal-dialog modal-xl modal-dialog-centered modal-dialog-scrollable">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title col-11 text-center"  id="divProcedimientosCTC">JUSTIFICACION PROCEDIMIENTOS NO POS</h5>
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			
			<div class="modal-body">
				<form id="FormProcedimientosCTC1" name="FormProcedimientosCTC1" class="needs-validation" novalidate>
					<div class="row pb-1">
						<div class="input-group">
							<div class="col-2{width: 80%}" style="padding-right: 0%; padding-left: 1%">
								<label type="hidden" id="lblProcNP" for="selProcedimientoNP">Procedimiento NO POS &nbsp; &nbsp; &nbsp; &nbsp;</label>
							</div>	
							<div class="col-10" style="padding-right: 2%; padding-left: 0%">
								<input class="form-control" id="ProcedimientoNP" name="procedimientoNP" type="text" value="" readonly>
							</div>
						</div>	
					</div>	
					
					<div class="row pb-1">
						<div class="col-12 col-sm-12 col-md-12 col-lg-5 col-xl-5">
							<div class="input-group">
								<div class="col-4" style="padding-right: 0%; padding-left: 1%">
									<label id="lblSolicitadoNP" for="selSolicitadoNP">Solicitado en:</label>
								</div>
								<div class="col-8">
									<select name="SolicitadoNP" class="custom-select" id="selSolicitadoNP">
									</select><br>
								</div>
							</div>
						</div>
					
						<div class="col-12 col-sm-12 col-md-12 col-lg-7 col-xl-7">
							<div class="input-group">
								<div class="col-4">
									<label id="lblObjetivoNP" for="selObjetivoNP">Objetivo:</label>
								</div>
								<div class="col-8">
									<select name="ObjetivoNP" class="custom-select" id="selObjetivoNP">
									</select><br>
								</div>
							</div>
						</div>
					</div>
					
					<div class="row pb-1">
						<div class="col-12 col-sm-12 col-md-12 col-lg-5 col-xl-5">
							<div class="input-group">
								<div class="col-4" style="padding-right: 0%; padding-left: 1%">
									<label id="lblCantidadNP" for="selCantidadNP">Cantidad: </label>
								</div>
								<div class="col-8">
									<input id="txtCantidadNP" name="CantidadNP" type="number" min="0" max="90" class="form-control mr-sm-2 w-50" readonly>
								</div>
							</div>
						</div>
					
						<div class="col-12 col-sm-12 col-md-12 col-lg-7 col-xl-7">
							<div class="input-group">
								<div class="col-4">
									<label id="lblRiesgoNP" for="selRiesgoNP">Riesgo:</label>
								</div>
								<div class="col-8">
									<select name="selRiesgoNP" class="custom-select" id="selRiesgoNP">
									</select><br>
								</div>
							</div>
						</div>
					</div>
					
					<div class="row pb-1">
						<div class="col-12 col-sm-12 col-md-12 col-lg-5 col-xl-5">
							<div class="form-group">
								<div class="custom-control custom-checkbox custom-control-inline">
									<input type="checkbox" class="custom-control-input" id="chkRiesgoNP" name="chkRiesgoNP">
									<label class="custom-control-label" for="chkRiesgoNP">Tipo de riesgo se encuentra soportado en historia clínica:</label>
								</div>
							</div>
						</div>
						
						<div class="col-12 col-sm-12 col-md-12 col-lg-7 col-xl-7">
							<div class="input-group">				
								<div class="col-12 col-sm-12 col-md-12 col-lg-9 col-xl-9">
									<label id="lblchkPacientePr" for="chkPacientePr">El paciente está informado de efectos secundarios y posibles riesgos:</label>
								</div>
								
								<div class="col-12 col-sm-12 col-md-12 col-lg-3 col-xl-3">
									<select id="chkPacientePr" name="chkPacientePr" class="custom-select" required>
										<option value=""></option> 
										<option value="S">Si</option>
										<option value="N">No</option>
									</select>
								</div>
							</div>
						</div>
					</div>
					
					<div class="form-row pb-2" id='divDiagnosticoOM'>
						<div class="col-12 col-sm-12 col-md-12 col-lg-12 col-xl-12">
							<div class="input-group input-group-sm">							
								<label id="lblCodigoCie" for="txtCodigoCieOM" class="required">Diagnóstico:</label>
								<label> &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; </label>
								<input type="text" class="form-control form-control-sm font-weight-bold ignore col-12 col-sm-12 col-md-12 col-lg-12 col-xl-12" name="txtCodigoCieOM" id="txtCodigoCieOM" autocomplete="off">
							</div>
						</div>
						
						<div class="input-group input-group-sm">
							<input type="text" class="form-control form-control-sm font-weight-bold col-12 col-sm-12 col-md-12 col-lg-1 col-xl-1" name="cCodigoCieOM" id="cCodigoCieOM" readonly="readonly">
							<input type="text" class="form-control form-control-sm font-weight-bold col-12 col-sm-12 col-md-12 col-lg-11 col-xl-11" name="cDescripcionCieOM" id="cDescripcionCieOM" readonly="readonly">
						</div>
					</div>
				</form>
					
				<form id="FormProcedimientosCTC2" name="FormProcedimientosCTC2" class="needs-validation" novalidate>
					<div class="row pb-1">					
						<div class="col-12">
							<div class="form-group">
								<div class="custom-control custom-checkbox custom-control-inline">
									<input type="checkbox" class="custom-control-input" id="chkExistePOSPr" name="chkExistePOSPr">
									<label class="custom-control-label" for="chkExistePOSPr">Existe Procedimiento POS?</label>
								</div>
							</div>
						</div>
					</div>
					
					<div class="form-row pb-2">
						<div class="input-group input-group-sm">
							<label for="lblProcedimientoP" for="selProcedimientoP">Procedimiento:</label>
							<label> &nbsp; &nbsp; &nbsp; </label>
							<input type="text" class="form-control form-control-sm font-weight-bold ignore col-12 col-sm-12 col-md-12 col-lg-12 col-xl-12" name="buscarProcedimientoP" id="buscarProcedimientoP" autocomplete="off" >
						</div>
						<div class="input-group input-group-sm">
							<input type="text" class="form-control form-control-sm font-weight-bold col-12 col-sm-12 col-md-12 col-lg-1 col-xl-1" name="codigoProcedimientoP" id="codigoProcedimientoP" readonly="readonly">
							<input type="text" class="form-control form-control-sm font-weight-bold col-12 col-sm-12 col-md-12 col-lg-11 col-xl-11" name="descripcionProcedimientoP" id="descripcionProcedimientoP" readonly="readonly">
						</div>
					</div>
					
					<div class="row pb-1">
						<div class="col-12 col-sm-12 col-md-12 col-lg-12 col-xl-12">
							<div class="input-group">
								<div class="col-2">
									<label id="lblCantidadP" for="selCantidadP">Cantidad:</label>
								</div>
								<div class="col-3">
									<input id="txtCantidadP" name="CantidadP" type="number" min="0" max="90" class="form-control mr-sm-2 w-50">
								</div>
							</div>
						</div>
					</div>
					
					<div class="row pb-1">
						<div class="col-12">
							<label type="hidden" id="lblRespuesta" for="selRespuesta">Respuesta Clínica y paraclínica alcanzada:</label>
						</div>	
					</div>	
					
					<div class="row pb-1">					
						<div class="col-12">
							<textarea type="text" class="form-control" id="edtRespuestaP" name="RespuestaP" ></textarea>
						</div>
					</div>
					<hr>					
				</form>
				
				<form id="FormProcedimientosCTC3" name="FormProcedimientosCTC3" class="needs-validation" novalidate>			
					<div class="row pb-1">					
						<div class="col-12">
							<label type="hidden" id="lblResumenP" for="selResumenP">Resumen de historia clínica que justifique el uso del servicio medico No Pos:</label>
						</div>	
					</div>	
					
					<div class="row pb-1">					
						<div class="col-12">
							<textarea type="text" class="form-control" id="edtResumenP" name="edtResumenP" ></textarea>
						</div>
					</div>
					
					<div class="row pb-1">
						<div class="col-12">
							<label type="hidden" id="lblBibliografiaP" for="edtBibliografiaP">Bibliografía:</label>
						</div>	
					</div>	
					
					<div class="row pb-1">
						<div class="col-12">
							<textarea type="text" class="form-control" id="edtBibliografiaP" name="BibliografiaP"></textarea>
						</div>
					</div>
				</form>
			</div>
				
			<div class="modal-footer">
				<button type="button" class="btn btn-light" data-dismiss="modal" data-backdrop="static" data-keyboard="false" aria-pressed="true" id="btnCancelaProcCTC"><u>C</u>ancelar</button>
				<button type="button" class="btn btn-primary" data-toggle="tooltip" data-placement="top" title="Guardar información" aria-pressed="true" id="btnGuardaProcCTC"><u>G</u>uardar</button>
			</div>
		</div>
	</div>
</div>
<script type="text/javascript" src="vista-comun/js/modalProcedimientosCTC.js"></script>
<script type="text/javascript" src="vista-comun/js/tiposRiesgo.js"></script>