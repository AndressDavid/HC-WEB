<div id="divObservaciones" class="modal hide" tabindex="-1" role="dialog" aria-labelledby="divObservaciones" data-backdrop="static not working">
	<div class="modal-dialog modal-xl modal-dialog-centered modal-dialog-scrollable">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title col-11 text-center"  id="divObservaciones">OBSERVACIONES ATENCIÓN URGENCIAS</h5>
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>

			<div class="modal-body">
				<div class="row pb-1">
					<div class="col" id="divPacObsUrg">
					</div>
				</div>
				<form id="FormObservaciones" name="FormObservaciones" class="needs-validation" novalidate>
					<div class="row pb-1">
						<div class="col-12">
							<div class="input-group">
                                <div class="col-12">
									<label id="lblObservaAnt" for="lblObservaAnt">Observaciones Anteriores:</label>
								</div>
								<div class="col-12">
					    		    <textarea rows="11" type="text" class="form-control" id="edtObservacionesAnt" name="edtObservacionesAnt" readonly></textarea>
						        </div>
							</div>
						</div>

						<div class="col-12">
							<div class="input-group">
                                <div class="col-12">
									<label id="lblObservacion" for="lblObservacion">Ingresar nueva observación :</label>
								</div>
								<div class="col-12">
					    		    <textarea rows="3" type="text" minlength="10" class="form-control" id="edtNuevaObserva" name="edtNuevaObserva"></textarea>
						        </div>
							</div>
						</div>
					</div>
				</form>
			</div>

			<div class="modal-footer">
				<button type="button" class="btn btn-light" data-dismiss="modal" data-backdrop="static" data-keyboard="false" aria-pressed="true" id="btnCancelaObs"><u>C</u>ancelar</button>
				<button type="button" class="btn btn-primary" data-toggle="tooltip" data-placement="top" title="Guardar información" aria-pressed="true" id="btnGuardaObs"><u>G</u>uardar</button>
			</div>
		</div>
	</div>
</div>
<script type="text/javascript" src="vista-comun/js/modalObservaciones.js"></script>